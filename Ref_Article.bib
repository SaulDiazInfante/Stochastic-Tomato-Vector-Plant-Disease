doi = {10.1186/s13662-015-0592-6},
file = {:home/gabrielsalcedo/Dropbox/Libros/Seminario/document.pdf:pdf},
issn = {1687-1847},
journal = {Advances in Difference Equations},
keywords = {Lyapunov function,exponential stability,persistent in mean,stochastic SIS epidemic model,vaccination},
month = {dec},
