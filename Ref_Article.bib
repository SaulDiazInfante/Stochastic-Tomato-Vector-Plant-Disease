pages = {328},
publisher = {Zhao et al.},
title = {{The asymptotic behavior of a stochastic SIS epidemic model with vaccination}},
url = {http://dx.doi.org/10.1186/s13662-015-0592-6 http://www.advancesindifferenceequations.com/content/2015/1/328},
volume = {2015},
year = {2015}
}